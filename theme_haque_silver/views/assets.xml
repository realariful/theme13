<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <template id="mystyle" name="My style" inherit_id="website.assets_frontend">
        <xpath expr="link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/theme_haque_silver/static/scss/style.scss"/>
        </xpath>
    </template>

    <template id="theme_editor" inherit_id="website.assets_editor" name="Theme Silver">
        <xpath expr="script[last()]" position="after">
          <script type="text/javascript" src="/theme_haque_silver/static/src/js/theme_editor.js" />
        </xpath>
      </template>

</odoo>